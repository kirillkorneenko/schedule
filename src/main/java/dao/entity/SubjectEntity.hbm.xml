<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.entity.SubjectEntity" table="subject" schema="schedule">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="nameSubject">
            <column name="nameSubject" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>