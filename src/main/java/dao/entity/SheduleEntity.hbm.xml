<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="dao.entity.SheduleEntity" table="shedule" schema="schedule">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="valide">
            <column name="valide" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="weekNumber">
            <column name="weekNumber" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <many-to-one name="daysweekByCodDayWeek" class="dao.entity.DaysweekEntity">
            <column name="codDayWeek"/>
        </many-to-one>
        <many-to-one name="callsByLessonNumber" class="dao.entity.CallsEntity">
            <column name="lessonNumber"/>
        </many-to-one>
        <many-to-one name="lecturehallByCodeAudience" class="dao.entity.LecturehallEntity">
            <column name="codeAudience"/>
        </many-to-one>
        <many-to-one name="groupsByCodeGroup" class="dao.entity.GroupsEntity">
            <column name="codeGroup"/>
        </many-to-one>
        <many-to-one name="userByCodeUserId" class="dao.entity.UserEntity">
            <column name="userId"/>
        </many-to-one>
        <many-to-one name="subjectByCodeDiscipline" class="dao.entity.SubjectEntity">
            <column name="codeDiscipline"/>
        </many-to-one>
        <many-to-one name="teacherByCodeTeacher" class="dao.entity.TeacherEntity">
            <column name="codeTeacher"/>
        </many-to-one>
    </class>
</hibernate-mapping>